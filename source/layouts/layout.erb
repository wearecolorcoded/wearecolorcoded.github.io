<!doctype html>
<html>
  <head>
    <meta charset="utf-8">

    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="apple-touch-icon" sizes="180x180" href="images/apple-touch-icon.png">
    <link rel="icon" type="image/ico" sizes="64x64" href="images/favicon.ico">
    <link rel="icon" type="image/png" sizes="32x32" href="images/favicon-32x28.png">
    <link rel="icon" type="image/png" sizes="16x16" href="images/favicon-16x14.png">
    <link rel="manifest" href="/manifest.json">
    <link rel="mask-icon" href="images/safari-pinned-tab.svg" color="#f5af13">
    <meta name="theme-color" content="#f5af13">

    <meta name="description" content="An initiative providing a series of creative events to empower People of Color in the tech industry. Sponsored by Vox Media Inc.">
    <link rel="canonical" href="http://colorcoded.voxmedia.com">
    <meta property="og:description" content="An initiative providing a series of creative events to empower People of Color in the tech industry. Sponsored by Vox Media Inc.">
    <meta property="fb:app_id" content="683221178510796">
    <meta property="og:image" content="http://colorcoded.voxmedia.com/images/css_workshop_banner.jpg">
    <meta property="og:site_name" content="Color Coded: Empowering People of Color in Tech">
    <meta property="og:title" content="Color Coded: Empowering People of Color in Tech">
    <meta property="og:type" content="website">
    <meta property="og:url" content="http://colorcoded.voxmedia.com">
    <!-- twitter -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:site" content="Color Coded">
    <meta name="twitter:creator" content="@pam_yam">
    <meta name="twitter:title" content="Empowering People of Color in Tech">
    <meta name="twitter:description" content="An initiative providing a series of creative events to empower People of Color in the tech industry. Sponsored by Vox Media Inc.">
    <meta name="twitter:image:src" content="http://colorcoded.voxmedia.com/images/css_workshop_banner.jpg">
    <title><%= current_page.data.title || "Color Coded: Empowering People of Color" %></title>

    <%= stylesheet_link_tag "all" %>
  </head>

  <body class="<%= page_classes %> container-fluid">
    <div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.8&appId=683221178510796";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
    <%= yield %>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
    <%= javascript_include_tag  "all" %>
  </body>
</html>
